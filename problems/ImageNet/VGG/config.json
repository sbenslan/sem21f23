{
    "experiment": {
        "n_folds": 1,
        "n_epochs": 70,
        "metrics": {
            "target_metric": "valid_loss"
        }
    },
    "data": {
        "augment": true,
        "bs_train": 48,
        "bs_valid": 48
    },
    "network": {
        "class": "VGG",
        "pretrained_unquantized": "https://download.pytorch.org/models/vgg19_bn-c79401a0.pth",
        "state_dict_conversion_unquantized": "vgg_torchvision_to_quantlab",
        "pretrained_quantized": null,
        "state_dict_conversion_quantized": null,
        "params": {
            "config": "VGG19",
            "bn": true
        },
        "quantize": {
            "routine": "features_ste_inq",
            "STE": {
                "n_levels": 255,
                "quant_start_epoch": 28
            },
            "INQ": {
                "n_levels": 3,
                "quant_init_method": "uniform-l2-opt",
                "quant_strategy": "magnitude"
            }
        }
    },
    "training": {
        "loss_function": {
            "class": "CrossEntropyLoss",
            "params": {}
        },
        "optimizer": {
            "class": "SGD",
            "params": {
                "lr": 0.005,
                "momentum": 0.9,
                "weight_decay": 5e-4
            }
        },
        "lr_scheduler": {
            "class": "HandScheduler",
            "params": {
                "schedule": {
                    "0": 0.2,
                    "3": 0.1,
                    "5": 1,
                    "7": 0.1,
                    "10": 1,
                    "17": 0.1,
                    "20": 1,
                    "27": 0.1,
                    "30": 1,
                    "37": 0.1,
                    "40": 1,
                    "47": 0.1,
                    "50": 1,
                    "57": 0.1,
                    "60": 1,
                    "65": 0.1
                }
            },
            "step_metric": "valid_metric"
        },
        "quantize": {
            "routine": "features_ste_inq_get_controllers",
            "STE": {
                "clear_optim_state_on_step": false
            },
            "INQ": {
                "schedule": {
                    "10": 0.2,
                    "20": 0.4,
                    "30": 0.6,
                    "40": 0.8,
                    "50": 0.9,
                    "60": 1.0
                },
                "clear_optim_state_on_step": true
            }
        }
    }
}
