{
    "experiment": {
        "n_folds": 1,
        "n_epochs": 90,
        "metrics": {
            "target_metric": "valid_loss"
        }
    },
    "data": {
        "augment": true,
        "bs_train": 48,
        "bs_valid": 48
    },
    "network": {
        "class": "VGG",
        "params": {
            "config": "VGG19",
            "bn": true
        },
        "quantize": null
    },
    "training": {
        "loss_function": {
            "class": "CrossEntropyLoss",
            "params": {}
        },
        "optimizer": {
            "class": "SGD",
            "params": {
                "lr": 0.01,
                "momentum": 0.9,
                "weight_decay": 5e-4
            }
        },
        "lr_scheduler": {
            "class": "HandScheduler",
            "params": {
                "schedule": {
                    "0": 1,
                    "25": 0.1,
                    "33": 1,
                    "39": 0.1,
                    "43": 1,
                    "49": 0.1,
                    "53": 1,
                    "59": 0.1,
                    "63": 1,
                    "69": 0.1,
                    "73": 1,
                    "79": 0.1
                }
            },
            "step_metric": "valid_metric"
        },
        "quantize": null

    }
}

